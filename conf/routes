
# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~
GET         /selectField               controllers.FieldEditorController.selectKey()
GET         /editfields/:key           controllers.FieldEditorController.editFields(key:String)
POST        /saveFields                controllers.FieldEditorController.save()
GET         /mappedValues              controllers.RawDataController.mappedItems()
#GET         /schedules                 controllers.RawDataController.schedules()
GET         /functions                 controllers.RawDataController.functions()
GET         /macros                    controllers.MacroNodeController.index()
GET         /macro/edit/:key           controllers.MacroNodeController.edit(key:MacroKey)
POST        /macro/save                controllers.MacroNodeController.save()
#GET         /messageMacros             controllers.RawDataController.messageMacros()
GET         /vocabulary                controllers.RawDataController.vocabulary()
GET         /downloadjson              controllers.IOController.downloadJson()
GET         /flow                      controllers.Flow2Controller.flow()
POST        /namedEdit                 controllers.NamedController.edit()
GET         /named                     controllers.NamedController.index()
GET         /dumpFields                controllers.RawDataController.dumpFields()
GET         /edit/:keyKind/:key        controllers.EditorController.edit(keyKind:net.wa9nnn.rc210.key.KeyKind, key:String)
GET         /edit/:keyKind             controllers.EditorController.editUnselected(keyKind:net.wa9nnn.rc210.key.KeyKind)
GET         /tabs                      controllers.LandingController.tabs()
POST        /schedule/save             controllers.ScheduleController.save()
GET         /                          controllers.LandingController.index()

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file              controllers.Assets.versioned(path="/public", file: Asset)
